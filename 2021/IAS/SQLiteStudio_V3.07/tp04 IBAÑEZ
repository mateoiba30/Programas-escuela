-- View: act 3,2
CREATE VIEW "act 3,2" AS SELECT NombreyApellido.Apellido, NombreyApellido.Nombre
FROM País INNER JOIN NombreyApellido
ON País.ID=NombreyApellido.ID

WHERE País.País=1

ORDER BY Apellido, Nombre;

